#---------------------------------------------------------------------------------------------

sub CompareOldBaseOldLoc
{
	my($inAppleGlotEnvPath, $inComponentName) = @_;

	#-----------------------------------------------------------------------------------------
	#	Check input path
	#-----------------------------------------------------------------------------------------

	if (!(-d "$inAppleGlotEnvPath"))
	{
		AALocUtilities::PrintLog("\n### ERROR: The path $inAppleGlotEnvPath doesn't exist.\n");
		return;
	}

