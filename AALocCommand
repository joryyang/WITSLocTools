# $gPcxPath = 'pcx';
# $gFpcxPath = 'fpcx';

# Figure out pcx and fpcx paths.
my $gPcxPath = '/usr/local/cbl/bin/pcx2';
my $gFpcxPath = '/usr/local/cbl/bin/fpcx';

unless ( -e $gPcxPath )
{
    $gPcxPath = `which pcx`;
    chomp $gPcxPath;
	
    $gFpcxPath = 'fpcx';
}

